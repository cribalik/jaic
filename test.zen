fn main() {
  var m: Monster;
  m.gold = 3;
  m.health = 10;
  m.weapon.damage = 20;
  m.weapon.durability = 30;

  print(m.gold, m.health, m.weapon.damage, m.weapon.durability);
}

struct Weapon {
  damage: int;
  durability: int;
}

struct Monster {
  gold: int;
  health: int;
  weapon: Weapon;
}
